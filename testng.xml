<?xml version="1.0" encoding="UTF-8"?>
<suite name="DemoWebShop CrossBrowser Suite" parallel="false" thread-count="3">
 <listeners>
        <listener class-name="utilities.ExtentListener"/>
    </listeners>
 
    <test name="Chrome Test">
        <parameter name="browser" value="chrome"/>
        <parameter name="os" value="windows"/>
        <classes>
           <class name="testcases.RegisterTest"/>
           <class name="testcases.LoginTest"/>
           <class name= "testcases.HomePageTest"/>
           <class name= "testcases.AddToCartTest"/>
        </classes>
    </test>

    <test name="Firefox Test">
        <parameter name="browser" value="firefox"/>
        <parameter name="os" value="windows"/>
        <classes>
            <class name="testcases.RegisterTest"/>
           <class name="testcases.LoginTest"/>
           <class name= "testcases.HomePageTest"/>
           <class name= "testcases.AddToCartTest"/>
        </classes>
    </test>

